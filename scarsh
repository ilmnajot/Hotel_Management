Random random = new Random();

        // Erkak va urg'ochi baliqlar soni random orqali aniqlanadi
        int maleCount = 1 + random.nextInt(5); // 1 dan 5 gacha erkak baliq
        int femaleCount = 1 + random.nextInt(5); // 1 dan 5 gacha urg'ochi baliq

        System.out.println("Akvariumda " + maleCount + " ta erkak va " + femaleCount + " ta urg'ochi baliq bor.");

        // Erkak baliqlarni yaratish
        for (int i = 0; i < maleCount; i++) {
            String name = "Erkak Baliq " + (i + 1);
            Fish maleFish = new Fish(name, "erkak");
            Fish.addFish(maleFish);
        }

        // Urg'ochi baliqlarni yaratish
        for (int i = 0; i < femaleCount; i++) {
            String name = "Urg'ochi Baliq " + (i + 1);
            Fish femaleFish = new Fish(name, "urg'ochi");
            Fish.addFish(femaleFish);
        }